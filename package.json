  "description": "List of official middleware for adonis-framework",
  "main": "index.js",
  "scripts": {
    "test": "npm run standard && istanbul cover _mocha --report lcovonly -- -R spec test && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
