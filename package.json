    "standard": "standard src/**/*.js --global use"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-middleware.git"
  },
  "keywords": [
    "adonis",
    "http-middleware",
    "middleware",
    "adonisjs"
  ],
  "author": "adonisjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-middleware/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-middleware#readme",
  "dependencies": {
    "bytes": "^2.3.0",
    "co-body": "^4.0.0",
    "csrf": "^3.0.0",
    "formidable": "^1.0.17",
    "lodash": "^4.2.1",
    "node-csp": "^1.0.1",
    "node-guard": "^1.0.0",
    "node-uuid": "^1.4.7"
  },
  "peerDependencies": {
    "adonis-fold": "^3.0.1"
  },
  "devDependencies": {
    "adonis-fold": "^3.0.1",
    "chai": "^3.4.1",
    "co": "^4.6.0",
    "co-mocha": "^1.1.2",
    "coveralls": "^2.11.6",
    "cz-conventional-changelog": "^1.1.5",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.0.0",
    "standard": "^5.4.1",
    "supertest": "^1.1.0"
